.terminal-news
	font-size 16px
	font-family: monospace;
	padding 25px 20px 20px 20px
	max-width: 1000px;
	h4
		font-weight bold
		margin 0 0 10px 0

	ul
		list-style none
		padding 0
		margin 0
		position relative
		text-align left
		text-align: left;

		li
			max-width: 670px;
	.bash
		color lighten(black, 45%)
	em
		font-weight bold
		display block
		margin 0 0 5px 0

	p
		display block
		margin 0 0 15px 0
		line-height: 20px;
